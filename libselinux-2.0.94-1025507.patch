From ce62e047b3b7a51bd6758464c170182afa7d92ed Mon Sep 17 00:00:00 2001
From: Miroslav Grepl <mgrepl@redhat.com>
Date: Mon, 30 Jun 2014 14:54:28 +0200
Subject: [PATCH 1/2] nscd permission support for netgroup

---
 libselinux/include/selinux/av_permissions.h | 2 ++
 libselinux/src/av_perm_to_string.h          | 2 ++
 2 files changed, 4 insertions(+)

diff --git a/libselinux/include/selinux/av_permissions.h b/libselinux/include/selinux/av_permissions.h
index b1e7860..3d73f52 100644
--- a/libselinux/include/selinux/av_permissions.h
+++ b/libselinux/include/selinux/av_permissions.h
@@ -873,6 +873,8 @@
 #define NSCD__SHMEMHOST                           0x00000080UL
 #define NSCD__GETSERV                             0x00000100UL
 #define NSCD__SHMEMSERV                           0x00000200UL
+#define NSCD__GETNETGRP                           0x00000400UL
+#define NSCD__SHMEMNETGRP                         0x00000800UL
 #define ASSOCIATION__SENDTO                       0x00000001UL
 #define ASSOCIATION__RECVFROM                     0x00000002UL
 #define ASSOCIATION__SETCONTEXT                   0x00000004UL
diff --git a/libselinux/src/av_perm_to_string.h b/libselinux/src/av_perm_to_string.h
index 59407e0..1763980 100644
--- a/libselinux/src/av_perm_to_string.h
+++ b/libselinux/src/av_perm_to_string.h
@@ -268,6 +268,8 @@
    S_(SECCLASS_NSCD, NSCD__SHMEMHOST, "shmemhost")
    S_(SECCLASS_NSCD, NSCD__GETSERV, "getserv")
    S_(SECCLASS_NSCD, NSCD__SHMEMSERV, "shmemserv")
+   S_(SECCLASS_NSCD, NSCD__GETNETGRP, "getnetgrp")
+   S_(SECCLASS_NSCD, NSCD__SHMEMNETGRP, "shmemnetgrp")
    S_(SECCLASS_ASSOCIATION, ASSOCIATION__SENDTO, "sendto")
    S_(SECCLASS_ASSOCIATION, ASSOCIATION__RECVFROM, "recvfrom")
    S_(SECCLASS_ASSOCIATION, ASSOCIATION__SETCONTEXT, "setcontext")
-- 
